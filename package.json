{
  "name": "heydev",
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "serve": "live-server . --watch ./dist",
    "start": "concurrently \"npm:watch\" \"npm:serve\"",
    "watch": "tsc --watch",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "live-server": "^1.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
